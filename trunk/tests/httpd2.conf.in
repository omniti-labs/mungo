LoadModule perl_module @@MODULEPATH@@/mod_perl.so
LoadModule authz_host_module @@MODULEPATH@@/mod_authz_host.so
LoadModule mime_module @@MODULEPATH@@/mod_mime.so
LoadModule dir_module @@MODULEPATH@@/mod_dir.so
Listen 8989
LockFile @@CHECKOUT@@/tests/logs/test.lock
ErrorLog @@CHECKOUT@@/tests/logs/error_log
PIDFile @@CHECKOUT@@/tests/logs/testrun.pid

PerlRequire @@CHECKOUT@@/tests/startup.pl
DocumentRoot @@CHECKOUT@@/tests/pages
DirectoryIndex index.asp
<Directory "@@CHECKOUT@@/tests/pages">
  AllowOverride None
  Options Indexes FollowSymlinks MultiViews
  Allow from all
</Directory>
<FilesMatch "\.asp$">
  PerlSetVar StatINC 1
  PerlSetVar Debug 1
  SetHandler perl-script
  PerlHandler Mungo
</FilesMatch>
